drop table memoboard;

create table memoboard(
id number(20) primary key,
name varchar2(200),
content varchar2(300),
time date
);

insert into memoboard values(1,'테스트제목','테스트내용',sysdate);
insert into memoboard values(2,'테스트제목2','테스트내용2',sysdate);
insert into memoboard values(3,'테스트제목3','테스트내용3',sysdate);
insert into memoboard values(4,'테스트제목4','테스트내용4',sysdate);
insert into memoboard values(5,'테스트제목5','테스트내용5',sysdate);

commit;

